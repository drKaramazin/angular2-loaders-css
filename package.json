{
  "name": "angular2-loaders-css",
  "version": "2.0.1",
  "description": "Angular 2 module for Loaders.css",
  "files": [
    "module/*.d.ts",
    "module/*.js",
    "module/*.js.map",
    "module/*.json"
  ],
  "main": "./index.js",
  "types": "./module/module.d.ts",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "ngc -p ./tsconfig.publish.json"
  },
  "keywords": [
    "angular5",
    "angular",
    "loaders.css",
    "spinner",
    "loader",
    "css",
    "module",
    "package"
  ],
  "author": {
    "name": "Paul Moff"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moff/angular2-loaders-css.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@angular/cli": "^6.2.9"
  },
  "dependencies": {
    "loaders.css": "^0.1.2",
    "rxjs-compat": "^6.5.3"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@angular/upgrade": "^6.1.10",
    "concurrently": "^5.0.0",
    "core-js": "^2.4.1",
    "lite-server": "^2.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "systemjs": "^0.19.47",
    "typescript": "2.9.2",
    "zone.js": "^0.10.2"
  }
}

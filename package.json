    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "ngc -p ./tsconfig.publish.json"
  },
  "keywords": [
    "angular5",
    "angular",
    "loaders.css",
    "spinner",
    "loader",
    "css",
    "module",
    "package"
  ],
  "author": {
    "name": "Nikolay Grishkin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drKaramazin/ngx-css-loaders2.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@angular/cli": "^7.3.9"
  },
  "dependencies": {
    "loaders.css": "^0.1.2",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@angular/upgrade": "^7.2.15",
    "concurrently": "^5.0.0",
    "core-js": "^2.4.1",
    "lite-server": "^2.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "systemjs": "^0.19.47",
    "typescript": "3.2.4",
    "zone.js": "^0.8.26"
  }

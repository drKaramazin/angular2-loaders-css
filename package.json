{
  "name": "angular2-loaders-css",
  "version": "2.0.1",
  "description": "Angular 2 module for Loaders.css",
  "files": [
    "module/*.d.ts",
    "module/*.js",
    "module/*.js.map",
    "module/*.json"
  ],
  "main": "./index.js",
  "types": "./module/module.d.ts",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "ngc -p ./tsconfig.publish.json"
  },
  "keywords": [
    "angular5",
    "angular",
    "loaders.css",
    "spinner",
    "loader",
    "css",
    "module",
    "package"
  ],
  "author": {
    "name": "Paul Moff"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moff/angular2-loaders-css.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "~5.0.0"
  },
  "dependencies": {
    "loaders.css": "^0.1.2"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/upgrade": "^5.0.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "lite-server": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.0",
    "systemjs": "^0.19.47",
    "typescript": "2.4.2",
    "zone.js": "^0.8.5"
  }
}

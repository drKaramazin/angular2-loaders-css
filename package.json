{
  "name": "ngx-css-loaders2",
  "version": "1.0.0",
  "description": "Angular module for Loaders.css",
  "files": [
    "module/*.d.ts",
    "module/*.js",
    "module/*.js.map",
    "module/*.json"
  ],
  "main": "./index.js",
  "types": "./module/module.d.ts",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
